<h1>Persona</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Per_nombre</th>
            <td>{{ entity.pernombre }}</td>
        </tr>
        <tr>
            <th>Per_apellido_paterno</th>
            <td>{{ entity.perapellidopaterno }}</td>
        </tr>
        <tr>
            <th>Per_apellido_materno</th>
            <td>{{ entity.perapellidomaterno }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ entity.password }}</td>
        </tr>
        <tr>
            <th>Salt</th>
            <td>{{ entity.salt }}</td>
        </tr>
        <tr>
            <th>Per_email</th>
            <td>{{ entity.peremail }}</td>
        </tr>
        <tr>
            <th>Isactive</th>
            <td>{{ entity.isActive }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_persona') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_persona_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_persona_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
