<h1>Persona list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Per_nombre</th>
            <th>Per_apellido_paterno</th>
            <th>Per_apellido_materno</th>
            <th>Username</th>
            <th>Password</th>
            <th>Salt</th>
            <th>Per_email</th>
            <th>Isactive</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('admin_persona_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.pernombre }}</td>
            <td>{{ entity.perapellidopaterno }}</td>
            <td>{{ entity.perapellidomaterno }}</td>
            <td>{{ entity.username }}</td>
            <td>{{ entity.password }}</td>
            <td>{{ entity.salt }}</td>
            <td>{{ entity.peremail }}</td>
            <td>{{ entity.isActive }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_persona_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_persona_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admin_persona_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
