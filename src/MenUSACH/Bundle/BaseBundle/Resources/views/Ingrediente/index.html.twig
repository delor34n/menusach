<h1>Ingrediente list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Ing_nombre</th>
            <th>Ing_categoria</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('ingrediente_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.ingnombre }}</td>
            <td>{{ entity.ingcategoria }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('ingrediente_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('ingrediente_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('ingrediente_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
