<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Under development{% endblock %}</title>
        {% stylesheets '@MenUSACHBaseBundle/Resources/public/less/*' filter='less,?yui_css' %}
    	<!--<link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />-->
        {% include 'BraincraftedBootstrapBundle::_stylesheets.html.twig' %}
        {% endstylesheets %}
        
        {% javascripts '@MenUSACHBaseBundle/Resources/public/js/*' filter='yui_js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <script src="http://code.jquery.com/ui/1.9.1/jquery-ui.js"></script>
        <script>
        $(function() {
            $( "#menu_form_men_fecha_inicio" ).datepicker();
            $( "#menu_form_men_fecha_termino" ).datepicker();
        });
        </script>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
        <h1>h1 Heading</h1>
        <h2>h2 Heading</h2>
        <h3>h3 Heading</h3>
        <h4>h4 Heading</h4>
        <h5>h5 Heading</h5>
        <h6>h6 Heading</h6>
        {% endblock %}
        {% block footer %}
        {% endblock %}
        
        {% block javascripts %}
        
        {% endblock %}
        {% include 'BraincraftedBootstrapBundle::_javascripts.html.twig' %}
    </body>
</html>
